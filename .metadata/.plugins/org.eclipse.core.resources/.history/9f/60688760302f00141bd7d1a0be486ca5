package pe.demo.registro.helper;

import pe.demo.registro.R;
import pe.demo.registro.activity.FormularioActivity;
import android.widget.EditText;
import android.widget.RatingBar;

public class FormularioHelper {
	private EditText nombre;
	
	public FormularioHelper(FormularioActivity activity) {
		nombre = (EditText)activity.findViewById(R.id.txtNombre);
		EditText site = (EditText)activity.findViewById(R.id.txtSite);
		EditText direccion = (EditText)activity.findViewById(R.id.txtDireccion);
		EditText telefono = (EditText)activity.findViewById(R.id.txtTelefono);
		RatingBar nota = (RatingBar)activity.findViewById(R.id.rbNotas);
	}
}
